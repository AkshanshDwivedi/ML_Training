   "source": [
    "import os\n",
    "os.listdir()\n",
    "f1=open(\"newFile.txt\",'w')\n",
    "for file in os.listdir():\n",
    "    if file.endswith(\".txt\"):\n",
    "        f = open(file)\n",
    "        for line in f:\n",
    "            f1.write(line)\n",
    "        f1.write(\"\\n\")\n",
    "        f.close()\n",
    "f1.close()"
   ]
